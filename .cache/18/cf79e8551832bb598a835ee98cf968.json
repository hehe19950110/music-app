nels{\n      width: 100vw;\n      height: calc(100vh - 240px);\n      display: flex;\n      /* overflow-x: scroll;*/\n      &.panel1 {\n        transform: translateX(0vw);\n        transition: transform .3s;\n      }\n      &.panel2 {\n        transform: translateX(-100vw);\n        transition: transform .3s;\n      }\n      .photo-panel {\n        width: 100vw;\n        height: 100%;\n        flex-shrink: 0;\n        .photo {\n          position: relative;\n          display: flex;\n          justify-content: center;\n          align-items: center;\n          height: 50vh;\n          margin-bottom: -20px;\n          div {\n            position: absolute;\n            width: 60vw;\n            height: 60vw;\n            top: 20px;\n            background-size: contain;\n          }\n          .photo-1 {\n            background: url(../svg/photo-1.svg) center no-repeat;\n            animation: rotate 10s linear infinite;\n          }\n          .photo-2 {\n            background: url(../svg/photo-2.svg) center no-repeat;\n            animation: rotate 10s linear infinite reverse;\n          }\n          .photo-3 {\n            background: url(../svg/photo-3.svg) center no-repeat;\n            animation: rotate 10s linear infinite;\n          }\n        }\n  \n        .lyric {\n          display: inline;\n          text-align: center;\n          flex-shrink: 0;\n          p {\n            font-size: 14px;\n            color: $color2;\n            &.current {\n              color: $color;\n              font-size: 16px;\n            }\n          }\n        }\n      }\n      .panel-lyrics {\n        width: 100vw;\n        height: 100%;\n        flex-shrink: 0;\n        text-align: center;\n        font-size: 16px;\n        .container{\n          transition: .3s;\n          transform: translateY(-200px);\n          p {\n            margin-top: 2px;\n            letter-spacing: 1px;\n            color: $color2;\n            &.current {\n              color: $color;\n          }  \n        }\n      }\n\n      }\n    }\n  }\n  .footer{\n    height: 140px;\n    overflow: hidden;\n    .buttons {\n      display: flex;\n      justify-content: center;\n      margin: 0 auto;\n      img {\n        width: 25px;\n        height: 25px;\n        margin: 0 20px;\n      }\n    }\n  \n    .bar-area {\n      color: $color2;\n      font-size: 14px;\n      display: flex;\n      padding: 0 20px;\n      height: 40px;\n      align-items: center;\n      .time-start {\n        order: 1;\n        width: 40px;\n      }\n      .time-end {\n        order: 3;\n        width: 40px;\n      }\n      .bar {\n        order: 2;\n        flex: 1;\n        height: 4px;\n        background: $color4;\n        border-radius: 2px;\n        margin: 0 10px;\n        .progress {\n          width: 0%;\n          transition: all .2s;\n          height: 100%;\n          border-radius: 2px;\n          background: $color3;\n          position: relative;\n          &::after {\n            content: '';\n            position: absolute;\n            right: -8px;\n            top: -7px;\n            display: block;\n            width: 16px;\n            height: 16px;\n            background: url(../svg/progress.svg) 0 0 no-repeat;\n            background-size: contain;\n          }\n        }\n      }\n    }\n  \n    .actions {\n      display: flex;\n      align-items: center;\n      justify-content: space-between;\n      height: 160px;\n      padding: 0 10px;\n      margin-left: 17px;\n      margin-top: 10px;\n      svg {\n        transform: translateX(10%);\n      }\n    }\n  }\n\n\n}\n\n@keyframes rotate {\n  0% {\n    transform: rotate(0);\n  }\n  100% {\n    transform: rotate(360deg);\n  }\n}"},"lineCount":null}},"error":null,"hash":"0e4aad304a126bc7bc793b70f7fff431","cacheData":{"env":{}}}